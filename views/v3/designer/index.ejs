<%- include('../common/header', { menus }); %>

<div class="breadcrumb-area pt-35 pb-35 bg-gray-3">
    <div class="container">
        <div class="breadcrumb-content text-center">
            <ul>
                <li>
                    <a href=<%= domain; %>>Home</a>
                </li>
                <li class="active"> Categories</li>
            </ul>
        </div>
    </div>
</div>

<div class="designers-filter col-12" style="padding: 20px;background-color: gray;">
    <div class="designers-search col-lg-3 col-md-3 col-sm-12 form-group" style="display: inline-block;">
        <input type="text" class="form-control keyword-search" placeholder="Find a designer" trk="549">
    </div>
    <div class="designers-menu col-lg-3 col-md-3 col-sm-12 form-group" style="display: inline-block;">
        <select class="form-control menu" id="sel1">
            <% let parentCategories= []; %>
            <% menus.forEach(function(menu){ %>
                <% parentCategories = (menu.objectId == menuId) ? menu.categories : parentCategories ;%>
                <option value="<%= menu.objectId; %>" 
                    <%= menu.objectId == menuId ? 'selected' : '' %>><%= menu.name; %></option>
            <% }); %>
        </select>
    </div>
    <div class="designers-parent-category col-lg-3 col-md-3 col-sm-12 form-group" style="display: inline-block;">
        <select class="form-control parent" id="sel1">
            <% parentCategories.forEach(function(parentCategory){ %>
                <option value="<%= parentCategory.objectId; %>" 
                    <%= parentCategory.objectId == parentId ? 'selected' : '' %>><%= parentCategory.name; %></option>
            <% }); %>
        </select>
    </div>
</div>

<div class="designers-result col-12 row" style="padding: 20px;background-color: lightgray;margin: 0 0 0 0;">
    
</div>

