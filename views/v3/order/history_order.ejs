<!doctype html>
<html class="no-js" lang="zxx">

<%- include('../common/head', {}); %>

<body>
<%- include('../common/header', { menus }); %>

<div class="breadcrumb-area pt-35 pb-35 bg-gray-3">
    <div class="container">
        <div class="breadcrumb-content text-center">
            <ul>
                <li>
                    <a href=<%= domain; %>>Home</a>
                </li>
                <li class="active">Page Order History</li>
            </ul>
        </div>
    </div>
</div>
<div class="cart-main-area pt-90 pb-100">
    <div class="container">
        <h3 class="cart-page-title">Your Orders</h3>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <form action="#">
                    <div class="table-content table-responsive cart-table-content">
                        <table>
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th>Status</th>
                                    <th>Total Price</th>
                                    <th> Detail </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% orders.forEach(function(order){ %>
                                    <tr>
                                        <td class="product-name product-quantity">
                                            <span><img style="width: 82px;" src=<%= domain + Object.values(order.items)[0].images; %> alt=""></span>
                                        </td>
                                        <td class="product-price-cart"><span class="amount"><%= order.status; %></span></td>
                                        <td class="product-price-cart">$<span class="amount"><%= order.amount; %></span></td>
                                        <td class="product-detail ">
                                            <a class="btn btn-primary" href="<%= domain + 'order/' + order.objectId; %>" role="button">Detail</a>
                                        </td>
                                        <!--<td class="product-remove">-->
                                            <!--<a href="#"><i class="fa fa-pencil"></i></a>-->
                                            <!--<a href="#"><i class="fa fa-times"></i></a>-->
                                        <!--</td>-->
                                    </tr>
                                <% }); %>
                            </tbody>
                        </table>
                    </div>
                </form>
            </div>
        </div>
        <div>
            <div class="pro-pagination-style text-center mt-30">
                <ul>
                    <% if (prePage > 0) { %>
                    <li><a class="prev" href=
                                <%= domain
                                + 'user/order/history?page=' + prePage
                                + '&limit=' + limit
                                + '&keyword=' + keyword %>>
                            <i class="fa fa-angle-double-left"></i></a>
                    </li>
                    <% if (prePage > 1) { %>
                    <li><a class="" href="#">...</a></li>
                    <% } %>
                    <li><a href= <%= domain
                        + 'user/order/history?page=' + prePage
                        + '&limit=' + limit
                        + '&keyword=' + keyword %>><%= prePage; %></a>
                    </li>
                    <% } %>

                    <% if (totalPage) { %>
                    <li><a class="active" href="#"><%= page; %></a></li>
                    <% } %>

                    <% if (nextPage <= totalPage) { %>
                    <li><a href=<%= domain
                        + 'user/order/history?page=' + nextPage
                        + '&limit=' + limit
                        + '&keyword=' + keyword %>><%= nextPage; %></a>
                    </li>
                    <% if (nextPage <= (totalPage - 1)) { %>
                    <li><a class="" href="#">...</a></li>
                    <% } %>
                    <li><a class="next" href=<%= domain
                        + 'user/order/history?page=' + nextPage
                        + '&limit=' + limit
                        + '&keyword=' + keyword %>>
                            <i class="fa fa-angle-double-right"></i></a>
                    </li>
                    <% } %>
                </ul>
            </div>

        </div>
    </div>
</div>
