<!doctype html>
<html class="no-js" lang="zxx">

<%- include('../common/head', {}); %>

<body>
<%- include('../common/header', { menus }); %>

<div class="breadcrumb-area pt-35 pb-35 bg-gray-3">
    <div class="container">
        <div class="breadcrumb-content text-center">
            <ul>
                <li>
                    <a href=<%= domain; %>>Home</a>
                </li>
                <li class="active">Page Order History</li>
            </ul>
        </div>
    </div>
</div>
<div class="cart-main-area pt-90 pb-100">
    <div class="container">
        <h3 class="cart-page-title">Your Orders Detail</h3>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <form action="#">
                    <div class="table-content table-responsive cart-table-content">
                        <table>
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th>Product Name</th>
                                    <th>Until Price</th>
                                    <th>Number</th>
                                    <th>Subtotal</th>
                                    <!--<th></th>-->
                                </tr>
                            </thead>
                            <tbody>
                            <% products.forEach(function(product){ %>
                            <tr>
                                <td class="product-thumbnail">
                                    <a href=<%= domain + 'product/' + product.objectId; %>>
                                        <img style="width: 82px;" src=<%= domain + product.images[0]; %> alt="">
                                    </a>
                                </td>
                                <td class="product-name product-quantity">
                                    <a href=<%= domain + 'product/' + product.objectId; %>><%= product.name %></a>
                                    <!-- <br>
                                            <span class="amount">$<%= product.price; %></span>
                                            <br>
                                            <div class="cart-plus-minus">
                                                <input class="cart-plus-minus-box" type="text" name="qtybutton" value=<%= product.count; %>>
                                            </div>
                                            <br>
                                            <span>$<%= product.price * product.count; %></span> -->
                                </td>
                                <td class="product-price-cart">
                                    <span class="amount"><%= product.price; %></span>
                                    &nbsp; VND
                                </td>
                                <td class="product-quantity">
                                    <span><%= product.count %></span>
                                </td>
                                <td class="product-subtotal">
                                    <span class="product-subtotal-<%= product.objectId; %>"><%= product.price * product.count; %></span>
                                    &nbsp; VND
                                </td>
                            </tr>
                            <% }); %>
                            </tbody>
                        </table>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
