                            <label class="btn btn-default btn-file no-padding mt-2 ml-1">
                              <span class="new-button-color">chose imange</span>

                              <input class="btn btn-default btn-file" type="file" id="image" name="image">
                            </label>
                          </div>
                          <div class="col-3 col-md-3">
                            <img id="blah1"
                                 src="data:image/png;base64,"
                                 width="100px" height="100px" alt="your image"/>

                            <label class="btn btn-default btn-file no-padding mt-2 ml-1">
                              <span class="new-button-color">chose imange</span>

                              <input class="btn btn-default btn-file" type="file" id="image1" name="image1">
                            </label>
                          </div>
                          <div class="col-3 col-md-3">
                            <img id="blah2"
                                 src="data:image/png;base64,"
                                 width="100px" height="100px" alt="your image"/>

                            <label class="btn btn-default btn-file no-padding mt-2 ml-1">
                              <span class="new-button-color">chose imange</span>

                              <input class="btn btn-default btn-file" type="file" id="image2" name="image2">
                            </label>
