<%- include('../common/head', {
csslinks: [
`http://netdna.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.css`,
`http://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.11/summernote.css`,
`${domain}assets/css/lib/chosen/chosen.min.css`
]
}); %>


<style>
  .btn-file {
    position: relative;
    overflow: hidden;
  }

  .btn-file input[type=file] {
    position: absolute;
    top: 0;
    right: 0;
    min-width: 100%;
    min-height: 100%;
    font-size: 100px;
    text-align: right;
    filter: alpha(opacity=0);
    opacity: 0;
    outline: none;
    background: white;
    cursor: inherit;
    display: block;
  }

  .content-submit {
    padding: 10px 10px 10px 0;
    border-top: 1px solid #e1e1e1;
    text-align: right;
    background-color: #f0f3f5;
  }

  .multi-select {
    border-radius: 1px;
  }

  .yellow {
    color: yellow;
  }

  .gray {
    color: gray;
  }

  .green {
    color: green;
  }

  .blue {
    color: blue;
  }

  .maroon {
    color: maroon;
  }

  .columns .chosen-container, .column .chosen-container {
    width: 100% !important;
    margin-bottom: 1rem;
  }

  .cropit-preview {
        background-color: #f8f8f8;
        background-size: cover;
        border: 1px solid #ccc;
        border-radius: 3px;
        margin-top: 7px;
        width: 300px;
        height: 400px;
      }
      .cropit-preview-image-container {
        cursor: move;
      }
      .cropit-preview-image-container img {
        max-width: none;
      }
      .image-size-label {
        margin-top: 10px;
      }
      input {
        display: block;
      }
      input.cropit-image-input {
  visibility: hidden;
}

.select-image-btn {
    background: #52a0c7;
    color: #fff;
    padding: 5px 10px;
    text-align: center;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    cursor: pointer;
    display: inline-block;
    position: relative;
    -webkit-box-shadow: 0 2px #3784aa;
    box-shadow: 0 2px #3784aa;
}

.image-editor {
  text-align: center;
}
  
</style>

<body class="">
<!-- Left Panel -->

<aside id="left-panel" class="left-panel">
  <%- include('../common/navbar', {}); %>
</aside>

<!-- /#left-panel -->
<!-- Right Panel -->
<div id="right-panel" class="right-panel">
  <!-- Header-->
  <%- include('../common/header', {}); %>
  <!-- /#header -->
  <!-- Content -->
  <div class="content">
    <!-- Animated -->
    <form id="updateFrom" action="" method="post" enctype="multipart/form-data">
      <div class="row">
        <input type="text" name="productId" value="<%= product.objectId; %>" style="display: none;">
        <div class="col-lg-12 col-md-12 col-sm-12">
          <div class="animated fadeIn card card-body card-block">
            <div class="col col-md-12">
              <label for="image1">Image (require: 400 < width < 800 and scale 3/4 and not greater than 2MB)</label>
              <a role="button" id="crop" data-target="#cropImage" class="btn btn-primary float-right demo-crop">Add new image</a>

            </div>

            <div class="col-12 col-md-12 images" style="min-height: 300px">
              <!-- <div class="pic col-lg-3 col-md-3 col-sm-6">
                <img class="default-image"
                  src="data:image/png;base64," alt="Smiley face">
              </div> -->
              <%if (product && product.images){%>
                <%product.images.forEach(function (image) { %>
                  <div class="img" 
                    style="background-image: url('<%= domain + image%>');" rel="'<%= domain + image%>'"><span>remove</span></div>'
                <%})%>
              <%}%>
            </div>
          </div>

        </div>
        <div class="col-lg-8 col-md-8 col-sm-12">
          <div class="animated fadeIn">
            <div class="row">
              <div class="col-lg-12">
                <div class="card">
                  <div class="card-body card-block">

                    <div class="form-group col-lg-6 col-md-6 col-sm-12">
                      <div class="col-lg-12">
                        <label for="shortDescription" class=" form-control-label">Short Description</label>
                        <textarea type="text" id="shortDescription" name="shortDescription"
                                  class="form-control"
                                  style="height: 150px;"><%= product.shortDescription ? product.shortDescription : '' %>
                        </textarea>
                      </div>
                    </div>

                    <div class="form-group col-lg-6 col-md-6 col-sm-12">
                      <div class="col-md-12">
                        <label for="Description" class=" form-control-label">Description</label>
                        <textarea type="text" id="Description" name="description"
                          class="form-control"
                          style="height: 150px;"><%= product.description ? product.description : '' %>
                        </textarea>
                      </div>
                    </div>

                    <div class="form-group col-lg-6 col-md-6 col-sm-12">
                      <div class="col-md-12 col-sm-12">
                        <label class=" form-control-label">Information</label></div>
                      <div class="col-md-12 col-sm-12">
                        <textarea id="summernote" name="information"
                                  width="100px">
                          <%- product.information ? product.information : '' %>
                        </textarea>
                      </div>
                    </div>


                    <div class="form-group col-lg-6 col-md-6 col-sm-12">
                      <div class="col-md-12 col-sm-12">
                        <label class=" form-control-label">User manual</label></div>
                      <div class="col-md-12 col-sm-12">
                        <textarea id="summernote1" name="userManual"
                                  width="100px">
                          <%- product.userManual ? product.userManual : ''%>
                        </textarea>
                      </div>
                    </div>

                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

        <div class="col-lg-4 col-md-4 col-sm-12">
          <div class="animated fadeIn">
            <div class="row">
              <div class="col-lg-12">
                <div class="card">
                  <div class="card-body card-block">
                    <div class="row form-group">
                      <div class="col col-md-12">
                        <label for="name" class=" form-control-label">Name</label>
                        <input type="text" id="name" name="name"
                               value="<%= product.name ? product.name : '' %>"
                               class="form-control">
                      </div>
                    </div>

                    <div class="row form-group">
                      <div class="col col-md-12">
                        <label for="price" class=" form-control-label">Price</label>
                        <input type="number"
                               id="price"
                               name="price"
                               value="<%= product.price ? product.price : 0 %>"
                               class="form-control">
                      </div>
                    </div>

                    <div class="row form-group">
                      <div class="col col-md-12">
                        <label for="price" class=" form-control-label">Sale Price</label>
                        <input type="number" id="price" name="salePrice" class="form-control" value="<%= product.salePrice ? product.salePrice : 0 %>">
                      </div>
                    </div>
                    <div class="row form-group">
                      <div class="col col-md-12">
                        <label for="quantity" class=" form-control-label">Quantity</label>
                        <input type="number"
                               id="quantity"
                               name="quantity"
                               value="<%= product.quantity ? product.quantity : 0 %>"
                               class="form-control">
                      </div>
                    </div>
                    <div class="row form-group">
                      <div class="col col-md-12">
                        <label for="linkFacebook" class=" form-control-label">Link FaceBook</label>
                        <input type="text"
                               id="linkFacebook"
                               name="linkFacebook"
                               value="<%= product.linkFacebook ? product.linkFacebook : '' %>"
                               class="form-control">
                      </div>
                    </div>
                    <div class="row form-group">
                      <div class="col col-md-12">
                        <label for="linkInstagram" class=" form-control-label">Link Instagram</label>
                        <input type="text"
                               id="linkInstagram"
                               name="linkInstagram"
                               value="<%= product.linkInstagram ? product.linkInstagram : '' %>"
                               class="form-control">
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-12 col-md-12">
                        <label for="fontSize" class=" form-control-label">Font Size</label>

                        <select id="fontSize" name="fontSize" data-placeholder="Choose size..." multiple
                                class="standardSelect">
                          <option value="" label="default"></option>
                          <% product.size.forEach(function(s){%>
                          <option value="S" <%= s == 'S' ? 'selected' : ''%>>Size S</option>
                          <option value="M" <%= s == 'M' ? 'selected' : ''%>>Size M</option>
                          <option value="L" <%= s == 'L' ? 'selected' : ''%>>Size L</option>
                          <option value="XL" <%= s == 'XL' ? 'selected' : ''%>>Size XL</option>
                          <option value="XXL" <%= s == 'XXL' ? 'selected' : ''%>>Size XXL</option>
                          <option value="XXXL" <%= s == 'XXL' ? 'selected' : ''%>>Size XXXL</option>
                          <%})%>
                        </select>
                      </div>

                      <div class="col-12 col-md-12 mt-3">
                        <label for="sizeNumber" class=" form-control-label">Size Number</label>
                        <select id="sizeNumber" name="sizeNumber" data-placeholder="Choose size..." multiple
                                class="standardSelect">
                          <% product.size.forEach(function(sn){%>
                          <option value="" label="default"></option>
                          <option value="35" <%= sn == '35' ? 'selected' : ''%>>Size 35</option>
                          <option value="36" <%= sn == '36' ? 'selected' : ''%>>Size 36</option>
                          <option value="37" <%= sn == '37' ? 'selected' : ''%>>Size 37</option>
                          <option value="38" <%= sn == '38' ? 'selected' : ''%>>Size 38</option>
                          <option value="39" <%= sn == '39' ? 'selected' : ''%>>Size 39</option>
                          <option value="40" <%= sn == '40' ? 'selected' : ''%>>Size 40</option>
                          <option value="41" <%= sn == '41' ? 'selected' : ''%>>Size 41</option>
                          <option value="42" <%= sn == '42' ? 'selected' : ''%>>Size 42</option>
                          <%})%>
                        </select>
                      </div>

                      <div class="col-12 col-md-12 mt-3">
                        <label for="color" class=" form-control-label">Color</label>
                        <select id="color" name="color" data-placeholder="Choose size..." multiple
                                class="standardSelect">
                          <% product.colors.forEach(function(cl){%>
                          <option value="" label="default"></option>
                          <option value="blue" class="blue" <%= cl == 'blue' ? 'selected' : ''%>>Blue</option>
                          <option value="maroon" class="maroon" <%= cl == 'maroon' ? 'selected' : ''%>>Maroon</option>
                          <option value="gray" class="gray" <%= cl == 'gray' ? 'selected' : ''%>>Gray</option>
                          <option value="green" class="green" <%= cl == 'green' ? 'selected' : ''%>>Green</option>
                          <option value="yellow" class="yellow" <%= cl == 'yellow' ? 'selected' : ''%>>Yellow</option>
                          <option value="black" class="black" <%= cl == 'black' ? 'selected' : ''%>>Black</option>
                          <option value="pink" class="pink" <%= cl == 'pink' ? 'selected' : ''%>>Pink</option>
                          <option value="white" class="white" <%= cl == 'white' ? 'selected' : ''%>>White</option>
                          <option value="orange" class="orange" <%= cl == 'orange' ? 'selected' : ''%>>Orange</option>
                          <option value="red" class="red" <%= cl == 'red' ? 'selected' : ''%>>Red</option>
                          <%})%>
                        </select>
                      </div>

                      <div class="col-12 col-md-12 mt-3">
                        <label for="time-delivery" class=" form-control-label">Time Delivery</label>
                        <input style="margin: 15px 0px 15px 0px;" type="number" id="delivery-from" name="deliveryFrom" placeholder="Delivery from" class="form-control" value="<%= product.deliveryFrom ? product.deliveryFrom : 0 %>">
                        <input style="margin: 15px 0px 15px 0px;" type="number" id="delivery-to" name="deliveryTo" placeholder="Delivery to" class="form-control" value="<%= product.deliveryTo ? product.deliveryTo : 0 %>">
                      </div>

                      <div class="col-12 col-md-12 mt-3">
                        <label for="category" class=" form-control-label">Categories</label>
                        <select multiple id="category" name="category" class="standardSelect">
                          <% if(listCategory && listCategory.length){ %>
                          <% product && product.categoryIds.forEach(function (id) {%>
                          <% listCategory.forEach(function(categories){ %>
                          <optgroup label="<%= categories.name %>">
                            <option style="display: none">&nbsp;</option>
                            <% categories.categories.forEach(function(category){ %>
                            <optgroup label="&nbsp;&nbsp;&nbsp;&nbsp;<%= category.name %>">
                              <% category.items.forEach(function(item){ %>
                              <option value="<%= `${categories.objectId}:${category.objectId}:${item.objectId}` %>" <%= id == item.objectId  ? 'selected' : ''%>>&nbsp;&nbsp;&nbsp;&nbsp; <%= item.name %></option>
                              <% }); %>
                            </optgroup>
                            <% }); %>
                            <% }); %>
                          </optgroup>
                          <%})%>
                          <% } %>
                        </select>
                      </div>

                      <div class="col-12 col-md-12 mt-5" >
                        <!-- <button type="submit" id="send" class="btn btn-sm btn-primary float-right"> -->

                        <button type="button" id="sendUpdate" class="col-12 col-md-12 mt-5 btn btn-outline-link" style="
                          background-color: #22A4E6;
                          padding: 10px 20px;
                          color: #fff;
                          border-radius: 3px;">Update</button>
                          <!-- <i class="fa fa-dot-circle-o"></i> Submit -->
                        <!-- </button> -->
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div><!-- .animated -->
        </div>
      </div>
    </form>
    <!-- .animated -->
  </div>


  <div  class="modal" id="cropImage" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <h5 class="modal-title" id="exampleModalLabel">Edit Image </h5>
        </div>
        <div class="modal-body">
          <div class="image-editor">
            <input type="file" class="cropit-image-input">
            <div class="cropit-preview" style="margin: auto;"></div>
            <div class="image-size-label">
              Resize image
            </div>
            <input type="range" class="cropit-image-zoom-input">
            <div class="btn select-image-btn">
              <span class="icon icon-image"></span>Select new image
            </div>
          </div>
      
  
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary"  data-dismiss="modal">Close</button>
          <button type="submit" class="btn btn-primary btn-save-new-menu export">Crop</button>
        </div>
      </div>
    </div>
  </div>

  <!-- /.content -->
  <div class="clearfix"></div>
  <!-- Footer -->
  <%- include('../common/footer', {}); %>
  <!-- /.site-footer -->
</div>



<!--end Crop image -->
